{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "DOM"],
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "paths": {
      "@ui/*": ["./packages/ui/*"],
      "@packages/*": ["./packages/*"],
      "@docs/*": ["./packages/docs/*"],
      "@root/*": ["./*"]
    },
    // "types": [
    //  "types-ol-ext"
    // ],
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true
    // "typeRoots": ["./node_modules/@types/", "./types"]
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist", "packages/docs", "packages/cli/lib"]
}
