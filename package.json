{
  "name": "surprise-ui",
  "version": "0.0.3",
  "description": "",
  "main": "dist/index.es.js",
  "module": "es/index.js",
  "types": "./types",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "build:ts": "tsup",
    "build:type": "tsup --dts-only --outDir=types",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "format": "set NODE_OPTIONS='--experimental-json-modules' && ts-node --esm ./scripts/format.mts",
    "prepare": "husky install",
    "clean": "rimraf ./dist",
    "lint-staged": "lint-staged",
    "commit": "cz",
    "changelog": "standard-changelog",
    "release": "standard-version"
  },
  "packageManager": "pnpm@7.28.0",
  "sideEffects": false,
  "author": "jinghong",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@commitlint/types": "^17.4.4",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chalk": "4.1.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "7.0.0",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "sass": "^1.62.0",
    "standard-changelog": "^2.0.27",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "^4.3.1",
    "vitest": "^0.30.1",
    "vue": "^3.2.47",
    "vue-tsc": "^1.4.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "peerDependencies": {
    "vue": ">= 3"
  },
  "files": [
    "dist",
    "types",
    "es"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
